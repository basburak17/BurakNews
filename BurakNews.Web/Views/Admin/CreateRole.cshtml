
@{
    ViewData["Title"] = "CreateRole";
}
@model string
<div class="container">
    <div class="row">
        <div class="col-md-6">
            <h1 class="text-center">CreateRole</h1>
            <form method="post" asp-action="CreateRole" asp-controller="Admin">
                <div class="form-group">
                    <label for="roleName">Rol Adı:</label>
                    <input class="form-control" type="text" id="roleName" name="roleName" />
                </div>
                <button class="btn btn-success" type="submit">Ekle</button>
            </form>
        </div>
    </div>
</div>
